{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "wwwroot/dist",
    "experimentalDecorators": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
