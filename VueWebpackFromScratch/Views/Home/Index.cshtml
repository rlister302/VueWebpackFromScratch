@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>


    <div id="app">
        <hello-world></hello-world>
    </div>
</div>


<script src="~/dist/main.bundle.js"></script>
<script src="~/dist/hello.bundle.js"></script>


<script>
    // This namespace of hello is
    // defined by the webpack entry point name
    // Pretty slick!
    hello.HelloWorld.DoLog();


    var x = new hello.HelloWorld();
    // Should see a message to the log that says "Doing class log!"
    x.DoClassLog();
</script>
